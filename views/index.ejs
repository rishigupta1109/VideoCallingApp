<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Meethub</title>
        <link rel="stylesheet" href="style.css">
        <script  src="/socket.io/socket.io.js"></script>
        <script  src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js"></script>
       
    <!-- <script src="https://enigmatic-everglades-57456.herokuapp.com/socket.io/socket.io.js"></script> -->
    
</head>
<body>
    <div class="navbar">
        <img src="logo.png" id="logo">
        <h1>Meethub</h1>
    </div>
    <div class="content">
        <div class="left">
            <h1>Video Meetings Free for Everyone</h1>
           
            <div class="buttons">
                <button id="create-meet-btn">Create Meeting</button>
                <div class="buttons">
                    <input type="text" id="roomid" placeholder="Meeting Code">
                    <button   id="join-meet-btn">Join Meeting</button>    
                </div>
            </div>
        </div>
        <div class="right">
            <img id="pic1" src="pic1.jpg" >
        </div>
    </div>
    <div class="room">
        <div class="videosection">
            <h1 class="roomid">Room ID : </h1>
            <div class="videogrid">
    
            </div>
            <div class="options"></div>
        </div>
        <div class="chatsection">
            <div class="chats"></div>
            <div class="input-chats">
                <input type="text" id="message">
                <button id="send">send</button>
            </div>
        </div>
    </div>
    <script src="app.js"></script>
    
 
</body>
</html>